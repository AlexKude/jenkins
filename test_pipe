#!groovy
// check jenkins_host properties
properties([disableConcurrentBuilds()])

pipeline {
  agent {
          label'master'
          }
      options {
              buildDiscarder(logRotator(numToKeepStr: '10', atifactNumToKeepStr: '10'))
              timestamps()
  }
  stages {
          stage("First_Step") {
                      steps {
                              sh 'ssh root@jenkins_host \'hostname\''
                              }
                         }
        }
}
